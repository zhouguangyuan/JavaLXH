 */
class Demon {
	private String name;
	static int count = 0;

	public Demon() {
		count++;
		System.out.println("Demon-" + count);


	}
}

public class StaticDemo07 {
	public static void main(String[] args) {
	 new Demon();
	 new Demon();
	 new Demon();
	 new Demon();
	 

	}

}
