package ch3.sec5;

/**
 * @author 作者 Kevin Zhou
 * @version 创建时间：2020年4月27日 上午11:12:34 类说明: 通过if...else语句判断一个数是奇数还是偶数
 */
public class IfElseDemo {
	public static void main(String[] args) {
		int x = 3;
		if (x % 2 == 1) {
			System.out.println("x是奇数");

		} else {
			System.out.println("x是偶数");
		}

	}
}
