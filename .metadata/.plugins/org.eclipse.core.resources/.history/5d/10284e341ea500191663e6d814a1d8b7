package ch4.kevin.com;
/**
 *二维数组的定义及使用
 * 
 */
public class ArrayDemo08 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int score [][] = new int[4][3];
		score[0][1] = 30 ;
		score[1][0] = 31 ;
		score[2][2] = 32 ;
		score[3][1] = 33 ;
		score[1][1] = 30 ;
		
		
	}

}
