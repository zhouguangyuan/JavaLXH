package ch6.sec9;
/** 
 * @author Kelvin 
 * @version 2020年9月22日 下午3:45:39 
 * @class instruction 
 */

abstract Person {
	private String name ;
	private int age;
	
	public Person(String name, int age){
      this.name = name;
      this.age = age;
	}
	
	public String getName(){
		return name;
	}
	
	public int getAge(){
		return age;
	}
	
	public void say(){
		System.out.println("Hello world!")
	}
}

class Student extends Person {
	
}

class Worker extends Person {
	
}
public class AbstractCaseDemo02 {

}
