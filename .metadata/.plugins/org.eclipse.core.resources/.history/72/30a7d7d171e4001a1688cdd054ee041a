package ch5.sec10;

/**
 * @author Kelvin
 * @version 2020年8月22日 下午8:15:27
 * @class instruction
 */
class Person {
	String name;
	int age;
	String country = "A city";

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}

	public String getCountry() {
		return country;
	}

	public void setCountry(String country) {
		this.country = country;
	}

	public String getInfo(){
		return "Name："+ this.getName()+ "; Age：" + this.getAge()+"; City:" + this.getCountry();
	}
}

public class StaticDemo01 {

}
