public class OperatorDemo05 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		if( 5 > 2){
			System.out.println("条件成立：5 > 2 ");
		}
		
		if(true)
		{
			System.out.println("直接写的true");
		}
		
		if( ( 3 + 6 ) == ( 3 -6 ) )
		{
			System.out.println("这是不可能成立的！");
		}

	}

}
