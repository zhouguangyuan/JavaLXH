package ch3.sec4;
/** 
 * @author 作者 Kevin Zhou 
 * @version 创建时间：2020年4月23日 上午10:21:48 
 * 类说明 
 */
public class OperatorDemo03 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int i = 10;
		int j = 3;
		
		System.out.println(i + " + " + j + " = " +( i + j ));
		System.out.println(i + " - " + j + " = " +( i - j ));
		System.out.println(i + " * " + j + " = " +( i * j ));
		System.out.println(i + " / " + j + " = " +( i / j ));

	}

}
