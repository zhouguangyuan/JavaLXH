package ch3.sec5;

/**
 * @author Kelvin
 * @version 2020年4月27日 下午3:02:34
 * @class instruction
 */
public class ForNestedDemo {
	public static void main(String[] args) {
        for( int i = 1; i <= 9; i++ ){
        	for( int j = 1; j <= i; j++ ){
        		System.out.print( i + "x" + j + " = " + (i*j) );
        	}
        }
	}
}
