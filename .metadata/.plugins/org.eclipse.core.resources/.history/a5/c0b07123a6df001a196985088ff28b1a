class Person {
	String name;
	int age;
	
	public Person() {
		// TODO Auto-generated constructor stub
	}

	public void setAge(int age) {
		this.age = age;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getAge() {
		return age;
	}

	public String getName() {
		return name;
	}
}

public class ClassDemo01 {
	
	public static void main(String[] args) {
		
	}

}
