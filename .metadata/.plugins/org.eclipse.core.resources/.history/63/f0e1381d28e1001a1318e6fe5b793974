package ch5.sec7;
/** 
 * @author Kelvin 
 * @version 2020年8月18日 下午3:51:53 
 * @class instruction 
 */
public class StringAPIDemo01 {
	
	public static void main(String[] args) {
		String str = "Hello";
		char c[] = str.toCharArray();
		
		for(int i = 0; i < c.length; i++)
		{
			System.out.print("c["+i+"]" + c[i]+"\t");
		}
	}

}
