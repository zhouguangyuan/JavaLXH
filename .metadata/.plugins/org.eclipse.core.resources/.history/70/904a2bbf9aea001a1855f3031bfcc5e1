package ch5.sec15;
/** 
 * @author Kelvin 
 * @version 2020年8月30日 下午4:27:18 
 * @class instruction 
 */
class Outer {
	private String info = "Hello world!";

	public String getInfo() {
		return info;
	}

	public void setInfo(String info) {
		this.info = info;
	}
	
	public void fun(){
		new Inner().print();
	}
	
	class Inner {
		public void print(){
			System.out.println(info);
		}
	}

}